{
  "modulesContent": {
    "$edgeAgent": {
      "properties.desired": {
        "schemaVersion": "1.0",
        "runtime": {
          "type": "docker",
          "settings": {
            "minDockerVersion": "v1.25",
            "loggingOptions": "",
            "registryCredentials": {
              "${CONTAINER_REGISTRY_USERNAME}": {
                "username": "${CONTAINER_REGISTRY_USERNAME}",
                "password": "${CONTAINER_REGISTRY_PASSWORD}",
                "address": "${CONTAINER_REGISTRY_SERVER}"
              },
              "docker": {
                "address": "docker.io",
                "username": "${CONTAINER_DOCKER_USERNAME}",
                "password": "${CONTAINER_DOCKER_PASSWORD}"
              }
            }
          }
        },
        "systemModules": {
          "edgeAgent": {
            "type": "docker",
            "settings": {
              "image": "mcr.microsoft.com/azureiotedge-agent:1.0.5",
              "createOptions": {
                "HostConfig": {
                  "Binds": [
                    "c:/config:/config"
                  ]
                }
              }
            }
          },
          "edgeHub": {
            "type": "docker",
            "status": "running",
            "restartPolicy": "always",
            "settings": {
              "image": "mcr.microsoft.com/azureiotedge-hub:1.0",
              "createOptions": {
                "HostConfig": {
                  "Binds": [
                    "c:/config:/config"
                  ],
                  "PortBindings": {
                    "5671/tcp": [{
                      "HostPort": "5671"
                    }],
                    "8883/tcp": [{
                      "HostPort": "8883"
                    }],
                    "443/tcp": [{
                      "HostPort": "443"
                    }]
                  }
                }
              }
            }
          }
        },
        "modules": {
          "TimescaleDB": {
            "version": "1.0",
            "type": "docker",
            "status": "running",
            "restartPolicy": "always",
            "settings": {
              "image": "${MODULES.TimescaleDb.debug}",
              "createOptions": {
                "HostConfig": {
                  "ExposedPorts": "5432/tcp",
                  "PortBindings": {
                    "5432/tcp": [{
                      "HostPort": "8081"
                    }]
                  }
                }
              }
            }
          },
          "NodeDataGenerator": {
            "version": "1.0",
            "type": "docker",
            "status": "running",
            "restartPolicy": "always",
            "settings": {
              "image": "${MODULES.NodeDataGenerator.debug}",
              "createOptions": {
                "HostConfig": {
                  "Binds": [
                    "c:/config:/config"
                  ],
                  "ExposedPorts": "8086/tcp",
                  "PortBindings": {
                    "8086/tcp": [{
                      "HostPort": "8086"
                    }]
                  }
                }
              }
            },
            "env": {
              "LOCAL_STORAGE_ACCOUNT_KEY": {
                "value": "${LOCAL_ACCOUNT_KEY}"
              },
              "LOCAL_STORAGE_ACCOUNT_NAME": {
                "value": "${LOCAL_ACCOUNT_NAME} "
              }
            }
          },
          "NodeDataRecorder": {
            "version": "1.0",
            "type": "docker",
            "status": "running",
            "restartPolicy": "always",
            "settings": {
              "image": "${MODULES.NodeDataRecorder.debug}",
              "createOptions": {
                "HostConfig": {
                  "Binds": [
                    "c:/config:/config"
                  ],
                  "ExposedPorts": "8087/tcp",
                  "PortBindings": {
                    "8087/tcp": [{
                      "HostPort": "8087"
                    }]
                  }
                }
              }
            },
            "env": {
              "LOCAL_STORAGE_ACCOUNT_KEY": {
                "value": "${LOCAL_ACCOUNT_KEY}"
              },
              "LOCAL_STORAGE_ACCOUNT_NAME": {
                "value": "${LOCAL_ACCOUNT_NAME} "
              }
            }
          }
        }
      }
    },
    "$edgeHub": {
      "properties.desired": {
        "schemaVersion": "1.0",
        "routes": {
          "NodeDataGeneratorToDataRecorder": "FROM /messages/modules/NodeDataGenerator/outputs/output1 INTO BrokeredEndpoint(\"/modules/NodeDataRecorder/inputs/input1\")"

        },
        "storeAndForwardConfiguration": {
          "timeToLiveSecs": 7200
        }
      }
    }
  }
}